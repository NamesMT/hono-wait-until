{
  "name": "hono-wait-until",
  "type": "module",
  "version": "2.0.1",
  "packageManager": "pnpm@10.17.1",
  "description": "",
  "author": "NamesMT <dangquoctrung123@gmail.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/namesmt",
  "homepage": "https://github.com/namesmt/hono-wait-until#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/namesmt/hono-wait-until.git"
  },
  "bugs": "https://github.com/namesmt/hono-wait-until/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs"
    }
  },
  "source": "./src/index.ts",
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=20.13.1"
  },
  "scripts": {
    "start": "NODE_ENV=dev tsx src/index.ts",
    "watch": "NODE_ENV=dev tsx watch src/index.ts",
    "stub": "unbuild --stub",
    "dev": "pnpm run watch",
    "play": "pnpm run stub && pnpm run --filter playground dev",
    "play:useBuild": "pnpm run build && pnpm run --filter playground dev",
    "lint": "eslint .",
    "test": "vitest",
    "test:types": "tsc --noEmit",
    "check": "pnpm lint && pnpm test:types && vitest run --coverage",
    "build": "unbuild",
    "release": "pnpm dlx changelogen@latest --release --push --publish",
    "prepare": "simple-git-hooks",
    "prepublishOnly": "pnpm run build"
  },
  "dependencies": {
    "hono": "^4.9.8",
    "wait-until-generalized": "^0.0.5"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.4.1",
    "@namesmt/utils": "^0.5.17",
    "@types/node": "^24.5.2",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.36.0",
    "lint-staged": "^16.2.0",
    "simple-git-hooks": "^2.13.1",
    "std-env": "^3.9.0",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "unbuild": "^3.6.1",
    "vitest": "^3.2.4"
  },
  "pnpm": {
    "overrides": {
      "is-core-module": "npm:@nolyfill/is-core-module@^1.0.39"
    },
    "onlyBuiltDependencies": [
      "esbuild",
      "simple-git-hooks",
      "unrs-resolver"
    ]
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
